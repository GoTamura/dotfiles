Config {
    font = "xft:MigMix1M:size=9:regular:antialias=true"
  , bgColor = "#121212"
  , fgColor = "#9fa8b1"
  , position = Top
  , lowerOnStart = True
  , hideOnStart = False
  , allDesktops = True
  , persistent = True
  , commands = [
    Run StdinReader
  , Run DynNetwork ["--template", "<dev>: <tx>kB/s <rx>kB/s"
    , "--Low", "1000" -- units: kB/s
    , "--High", "5000" -- units: kB/s
    , "--low", "darkgreen"
    , "--normal", "darkorange"
    , "--high", "darkred"
    ] 10
  , Run MultiCpu ["--template", "<total0>% <total1>%"
    , "--Low", "50" -- units: %
    , "--High", "85" -- units: %
    , "--low", "darkgreen"
    , "--normal", "darkorange"
    , "--high", "darkred"
    ] 10
  , Run CoreTemp ["--template", "<core0> <core1>"
    , "--Low", "70" -- units: 
    , "--High", "80" -- units: 
    , "--low", "darkgreen"
    , "--normal", "darkorange"
    , "--high", "darkred"
    ] 50
  , Run Memory ["--template", "<usedratio>%"
    , "--Low", "20" -- units: %
    , "--High", "90" -- units: %
    , "--low", "darkgreen"
    , "--normal", "darkorange"
    , "--high", "darkred"
    ] 10
  , Run BatteryP["BAT0"] ["--template", "<acstatus>"
    , "--Low", "10" -- units: %
    , "--High", "80" -- units: %
    , "--low" , "darkred"
    , "--normal", "darkorange"
    , "--high", "darkgreen"
    , "--"
    , "-o", "<left>% (<timeleft>)"
    , "-O", "<fc=#dAA520>Charging</fc>"
    , "-i", "<fc=#006000>Charged</fc>"
  ] 50
  , Run Com "acpi" [] "battery" 300
  , Run Date "%F %a %T" "date" 10
  ]
  , sepChar = "%"
  , alignSep = "}{"
  , template = "%StdinReader%}{%multicpu% %coretemp% %memory% %dynnetwork% %date% %battery%                 "
}
